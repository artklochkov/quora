    "def create_optimizer(loss):\n",
    "    optimizer = tf.train.AdamOptimizer()\n",
    "    minimize = optimizer.minimize(loss)\n",
    "    return minimize\n",
    "\n",
    "minimize = create_optimizer(log_loss)"
